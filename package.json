{
  "name": "gmoqai-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "set DISABLE_ESLINT_PLUGIN=true && next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint",
    "lint:styles": "stylelint 'src/**/*.module.css' --fix",
    "type-check": "tsc --noEmit",
    "create:component": "node scripts/create-component.js",
    "migrate:component": "node scripts/migrate-component.js",
    "test:visual": "npm run build && npm run start"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^6.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.0.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.4.0",
    "stylelint": "^16.0.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
